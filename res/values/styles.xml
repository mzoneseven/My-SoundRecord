<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="RecoderSeekBar" parent="@android:style/Widget.SeekBar">
        <item name="android:indeterminateOnly">false</item> 
        <item name="android:maxHeight">3dp</item>
        <item name="android:minHeight">3dp</item>
        <item name="android:thumb">@drawable/play_dot</item>
        <item name="android:thumbOffset">0dp</item>
        <item name="android:progressDrawable">@drawable/seekbarlayer</item>
        
    </style>
    
   <style name="V5_Theme_Holo_Light.Dialog.Alert">
       
        <item name="android:windowBackground">@drawable/dialog_full_holo_light</item>
    
        <item name="android:windowContentOverlay">@null</item>
        
        <item name="*android:windowMinWidthMajor">@dimen/dialog_min_width_major</item>
        <item name="*android:windowMinWidthMinor">@dimen/dialog_min_width_minor</item>
        <item name="android:windowTitleStyle">@style/V5_DialogWindowTitle_Holo_Light</item>
    </style>
    
    <style name="V5.Widget.Holo.Light.Button.Borderless.Small" parent="@*android:style/Widget.Holo.Light.Button.Borderless.Small">
        <item name="android:textSize">14sp</item>
    </style>
    
    
       <style name="V5_Theme_Holo_Light" parent="@android:Theme.Holo.Light"/>
    
       <style name="V5_Theme_Holo_Light.Dialog">
        <item name="*android:dialogTitleDecorLayout">@layout/dialog_title_holo</item>
        <item name="android:alertDialogTheme">@style/V5_Theme_Holo_Light.Dialog.Alert</item>
        <item name="android:windowTitleStyle">@style/V5_DialogWindowTitle_Holo_Light</item>
        <item name="android:borderlessButtonStyle">@style/V5.Widget.Holo.Light.Button.Borderless.Small</item>

        <item name="android:windowFrame">@null</item>
        <item name="android:windowBackground">@drawable/dialog_full_holo_light</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowSoftInputMode">stateUnspecified|adjustPan</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowActionModeOverlay">true</item>
        <item name="android:windowCloseOnTouchOutside">@*android:bool/config_closeDialogWhenTouchOutside</item>

        <item name="android:colorBackgroundCacheHint">@null</item>

        <item name="android:buttonBarStyle">@android:style/Holo.Light.ButtonBar.AlertDialog</item>


        <item name="android:listPreferredItemPaddingLeft">16dip</item>
        <item name="android:listPreferredItemPaddingRight">16dip</item>

        <item name="*android:preferencePanelStyle">@*android:style/PreferencePanel.Dialog</item>

<!--          <item name="android_windowContentOverlay">@null</item> -->

        <item name="android:windowAnimationStyle">@style/DialogAnimationStyle</item>
 
        <item name="*android:windowMinWidthMajor">@dimen/dialog_min_width_major</item>
        <item name="*android:windowMinWidthMinor">@dimen/dialog_min_width_minor</item>
    </style>
    
    <style name="DialogAnimationStyle">  
        <item name="android:windowEnterAnimation">@anim/panel_enter</item> 
        <item name="android:windowExitAnimation">@null</item>
     </style>
     
   <!-- 
   @android:style/DialogWindowTitle.Holo.Light"
    -->
    <style name="V5_DialogWindowTitle_Holo_Light" parent="@*android:style/DialogWindowTitle.Holo.Light">
        <item name="android:textAppearance">@style/V5.TextAppearance.Holo.Light.DialogWindowTitle</item>
        <item name="android:textColor">@color/dialog_title_text_color_holo_light</item>
    </style>
    
    <!-- 
   <style name="V5.TextAppearance.Holo.Light.DialogWindowTitle" parent="@android:style/TextAppearance.Holo.DialogWindowTitle">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/holo_black_light_primary</item>
    </style>
    -->
    <style name="V5.TextAppearance.Holo.Light.DialogWindowTitle" parent="@*android:style/TextAppearance.Holo.Light.DialogWindowTitle">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/holo_black_light_primary</item>
    </style>
    
       <!-- 
       parent="@android:style/V5.Theme"
        -->

    <style name="Theme.RecorderV5StyleNew" parent="@android:Theme.Holo.Light">
         
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionModeOverlay">true</item>

        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:windowTranslucentNavigation">true</item>
    </style>
    
